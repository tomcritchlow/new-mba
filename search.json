---
layout: none
---
[
  {% for post in site.posts %}
    {
      "title": "{{ post.title | escape }}",
      "url": "{{ site.baseurl }}{{ post.url }}",
      "date": "{{ post.date }}",
      "content": "{{ post.content | strip_html | escape }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]
